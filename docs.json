{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "Metafy API Dev",
  "colors": {
    "primary": "#806506",
    "dark": "#806506",
    "light": "#FCD23E"
  },
  "favicon": {
    "light": "./favicon.ico",
    "dark": "./favicon-light.ico"
  },
  "styling": {
    "codeblocks": "system"
  },
  "fonts": {
    "heading": {
      "family": "ClashDisplay",
      "source": "https://static.metafy.gg/fonts/ClashDisplay/ClashDisplay-Regular.otf",
      "format": "woff2",
      "weight": 400
    },
    "body": {
      "family": "SuisseIntl",
      "source": "https://static.metafy.gg/fonts/Suisse/SuisseIntl-Regular-WebM.woff2",
      "format": "woff2",
      "weight": 400
    }
  },
  "appearance": {
    "default": "system",
    "strict": false
  },
  "background": {
    "decoration": "windows"
  },
  "api": {
    "playground": {
      "display": "simple"
    },
    "examples": {
      "languages": ["curl", "javascript", "go"]
    }
  },
  "seo": {
    "indexing": "navigable"
  },
  "search": {
    "prompt": "Search the docs..."
  },
  "navigation": {
    "versions": [
      {
        "version": "v1",
        "groups": [
          {
            "group": "Documentation",
            "pages": ["api-reference/v1/authentication", "api-reference/v1/rate-limits"]
          },
          {
            "group": "Reference",
            "openapi": {
              "source": "./api-reference/v1/openapi-v1.yml",
              "directory": "v1"
            }
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Metafy",
          "href": "https://metafy.gg",
          "icon": "globe"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.mfy.gg",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "https://static.metafy.gg/logo/Black.svg",
    "dark": "https://static.metafy.gg/logo/White.svg",
    "href": "https://dev.metafy.gg"
  },
  "navbar": {
    "links": []
  },
  "footer": {
    "socials": {
      "discord": "https://discord.mfy.gg",
      "x": "https://x.com/TryMetafy",
      "github": "https://github.com/metafy-gg",
      "linkedin": "https://linkedin.com/company/trymetafy"
    }
  }
}
